<script>

function onebox_show_code_line_number (){
	$("a.show-lines").hide();
	$("table.nolines").each(function (a,b){
	   $(b).removeClass("nolines");
	   $(b).find("td.line-number").each (function (c,d){
	      console.log(d)
	      if ($(d).attr("line_number")){
	        $(d).html( $(d).attr("line_number") + ":" );
	      }
	  });
  });
}

Em.run.later(this,function (){

$("table.nolines").each(function (a,b){
 $(b).removeClass("nolines");
 $(b).find("td.line-number").each (function (c,d){
console.log(d)
  if ($(d).attr("line_number")){
    $(d).html( $(d).attr("line_number") + ":" );
  }
 });
});

},1000);
</script>


<a class="show-lines" onclick="onebox_show_code_line_number()">Show Lines</a>
<pre><code class="{{lang}}" style="white-space:normal"><table cellpadding="0" cellspacing="0" class="lines nolines"><tbody>{{/has_lines}}{{#lines}}<tr{{#selected}} class="selected"{{/selected}}{{#selected}} style="background:#cfc"{{/selected}}><td line_number="{{line_number}}" class="line-number{{#selected}} selected{{/selected}}" style='padding:0px 5px 0px 5px'></td><td class="line-data" style='padding-left:5px;'><div class="line{{#selected}} selected{{/selected}}">{{data}}</div></td></tr>{{/lines}}{{#has_lines}}</tbody></table>{{/has_lines}}</code></pre>



<style>
pre.onebox ol { background:#EBEBEB;
    padding-top:5px;
    padding-bottom:5px;
    overflow:auto;
    font-family:"Courier New",Courier,mono; margin:0;
    color:#fff; 
    word-break: break-word;
    padding-left:45px;
    margin:0;
} 
pre.onebox ol li { background:#fff; font-size:small;color:#000;
    border-bottom:1px solid #E3E3E3;
    width:99%;
      padding-left:5px;
}
pre.onebox ol {
    counter-reset: li-counter;
    list-style-type: none;
}
pre.onebox ol li.selected{
	background-color:#F8EEC7;
}
pre.onebox ol li:before {
    background-color:#cfcfcf;
    position: absolute;
	left: 31px;
	width: 40px;
    display:inline-block;
    color:#fff;
    text-align:right;
    font-weight: bold;
    text-align: right;
    z-index: 1;
    overflow: hidden;
    content: counter(li-counter);
    counter-increment: li-counter;
    
}
</style>
<pre class='onebox' style='white-space: normal'>
